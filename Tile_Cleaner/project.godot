; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "BitmaskEdgesData",
"language": "GDScript",
"path": "res://addons/Tile_Cleaner/Bitmask_Edges_Data.gd"
}, {
"base": "Resource",
"class": "TilePattern",
"language": "GDScript",
"path": "res://addons/Tile_Cleaner/Tile_Pattern.gd"
} ]
_global_script_class_icons={
"BitmaskEdgesData": "res://addons/Tile_Cleaner/Bitmask_Edges_Icon.png",
"TilePattern": "res://addons/Tile_Cleaner/Autotile_Ruleset_Icon.png"
}

[application]

config/name="Tile Cleaner"
run/main_scene="res://Slopes_Example/Slopes_Test_Map.tscn"
config/icon="res://addons/Tile_Cleaner/Tile_Cleaner_Icon.png"

[debug]

gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[editor_plugins]

enabled=PoolStringArray( "Tile_Cleaner" )

[rendering]

environment/default_environment="res://default_env.tres"
